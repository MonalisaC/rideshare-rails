<h1>Passengers#index</h1>
<!-- <p>Find me in app/views/passengers/index.html.erb</p> -->
<h2> Passengers List </h2>
<ul>
  <% @passengers.each do |passenger| %>
  <li>
    <span class="pass-name">
      <%= link_to passenger.name, passenger_path(passenger.id),
      class: 'pass-name-link'  %>.
    </span>
        Phone:  <%="#{passenger.phone_num}"%>
  </li>
  <% end%>
</ul>
